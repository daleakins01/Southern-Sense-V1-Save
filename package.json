{
  "name": "southernsense-website",
  "version": "1.0.0",
  "description": "Southern Sense Handcrafted Luxury Candles",
  "main": "main.js",
  "scripts": {
    "start": "npm-run-all --parallel watch:*",
    "watch:css": "npx tailwindcss -i ./src/input.css -o ./src/output.css --watch",
    "watch:eleventy": "npx @11ty/eleventy --serve",
    "build:css": "npx tailwindcss -i ./src/input.css -o ./src/output.css --minify",
    "build:eleventy": "npx @11ty/eleventy",
    "build": "npm-run-all build:css build:eleventy",
    "postinstall": "npx update-browserslist-db@latest"
  },
  "keywords": [
    "candles",
    "ecommerce",
    "tailwind",
    "eleventy"
  ],
  "author": "Paula Bryan",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "autoprefixer": "^10.4.19",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3"
  },
  "dependencies": {
    "firebase-admin": "^13.5.0"
  }
}
